    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "lovable-tagger": "^1.1.11",
    "postcss": "^8.5.6",
    "react-snap": "^1.23.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^5.4.19"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": true,
    "skipThirdPartyRequests": true,
    "removeBlobs": true,
    "inlineCss": false,
    "cacheAjaxRequests": false,
    "preconnectThirdParty": false,
    "http2PushManifest": false,
    "removeStyleTags": false,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "puppeteerExecutionTimeout": 120000,
    "timeout": 90000,
    "viewport": {
      "width": 1920,
      "height": 1080
    },
    "include": [
      "/",
      "/services",
      "/about",
      "/geography",
      "/reviews",
      "/contacts",
      "/faq",
      "/privacy",
      "/news",
      "/services/sea",
      "/services/aviation",
      "/services/auto",
      "/services/railway",
      "/services/consolidated",
      "/services/customs",
      "/services/warehouse",
      "/services/insurance"
    ]
  }
